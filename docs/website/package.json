{
  "name": "docs-docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run docs:generate && docusaurus start",
    "build": "npm run docs:generate && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "download:host-tools": "cmake -E env cmake -DQAR_BUILD_DIR='../../build/' -P ../../qar-streaming/cmake/HostTools.cmake",
    "docs:xml:qar-streaming-c": "cmake -E chdir ./doxygen/qar-streaming-c ../../../../build/tools/doxygen/doxygen",
    "docs:xml:qar-streaming": "cmake -E chdir ./doxygen/qar-streaming ../../../../build/tools/doxygen/doxygen",
    "docs:convert:c": "npx doxygen2docusaurus --id qar-streaming-c",
    "docs:convert:cpp": "npx doxygen2docusaurus --id qar-streaming",
    "docs:convert": "npm run docs:convert:c && npm run docs:convert:cpp",
    "docs:xml": "npm run docs:xml:qar-streaming-c && npm run docs:xml:qar-streaming",
    "docs:generate": "npm run download:host-tools && npm run docs:xml && npm run docs:convert"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@docusaurus/theme-search-algolia": "^3.9.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@xpack/doxygen2docusaurus": "2.0.0",
    "doxygen": "0.4.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
